    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "code": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "price": {
      "type": "decimal",
      "min": 0,
      "default": 0
    },
    "capcity": {
      "type": "integer",
      "default": 0,
      "required": true,
      "min": 0
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date",
      "required": true
    },
    "course": {
      "model": "course",
      "via": "classes"
    },
    "lessons": {
      "collection": "lesson",
      "via": "class"
    },
    "users": {
      "private": true,
      "plugin": "users-permissions",
      "collection": "user",
      "via": "classes",
      "dominant": true
    },
    "user_profiles": {
      "private": true,
      "collection": "user-profile",
      "via": "classes",
      "dominant": true
    },
    "e_learning": {
      "via": "classes",
      "model": "e-learning"
    }
  }
