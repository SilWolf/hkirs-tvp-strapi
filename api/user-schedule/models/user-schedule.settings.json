{
  "kind": "collectionType",
  "collectionName": "user_schedules",
  "info": {
    "name": "UserSchedule",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": false
  },
  "attributes": {
    "subject": {
      "type": "string",
      "required": true,
      "default": "(未命名的活動)"
    },
    "content": {
      "type": "text"
    },
    "startAt": {
      "type": "datetime",
      "required": true
    },
    "endAt": {
      "type": "datetime",
      "required": true
    },
    "user": {
      "private": true,
      "plugin": "users-permissions",
      "model": "user",
      "via": "user_schedules"
    }
  }
}
