{
  "name": "hkirs-tvp-strapi",
  "private": true,
  "version": "0.1.0",
  "description": "HKIRS TVP CMS built with Strapi",
  "scripts": {
    "develop": "strapi develop",
    "start": "strapi start",
    "build": "strapi build",
    "strapi": "strapi",
    "dev": "yarn develop",
    "lint": "echo 'skip lint' && exit 0",
    "test": "echo 'skip test' && exit 0",
    "docker:up": "docker-compose -f docker-compose.dev.yml up -d",
    "docker:start": "docker-compose -f docker-compose.dev.yml start",
    "docker:stop": "docker-compose -f docker-compose.dev.yml stop",
    "docker:down": "echo 'This will entirely remove docker container. If you confirm to do so, copy and execute the following command:\u001b[33m docker-compose -f docker-compose.dev.yml down\u001b[37m' && exit 0",
    "strapi:dump": "strapi config:dump -f ./.strapi/config.json",
    "strapi:restore": "strapi config:restore -f ./.strapi/config.json"
  },
  "devDependencies": {},
  "dependencies": {
    "strapi": "3.4.5",
    "strapi-admin": "3.4.5",
