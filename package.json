    "strapi-plugin-content-type-builder": "3.4.5",
    "strapi-plugin-email": "3.4.5",
    "strapi-plugin-upload": "3.4.5",
    "strapi-plugin-users-permissions": "3.4.5",
    "strapi-provider-upload-s3-plus": "^0.1.1",
    "strapi-utils": "3.4.5"
  },
  "author": {
    "name": "A Strapi developer"
  },
